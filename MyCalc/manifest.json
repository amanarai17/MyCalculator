{    
"name": "MyCalc",    
"version": "1.0",    
"description": "MY Calculator Extension for Chrome",    
"manifest_version": 2,
"icons":{"128": "icon.png"},
"browser_action": {
  "default_icon": "icon.png",

  "default_popup": "index.html"
},
"permissions": ["activeTab"],
"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}